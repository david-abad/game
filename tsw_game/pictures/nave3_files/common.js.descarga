function applyListFlex(containerClass, elementClass, height, truncate, rows){
    consoleLog("reordering: " + containerClass + ", height: " + height);

    if(rows != null){
        $("." + containerClass).each(function() {
            $(this).flexImages({
                //rowHeight: height,
                truncate: truncate,
                maxRows: rows,
                container: "." + elementClass
            })
        });
    } else{
        $("." + containerClass).each(function() {
            $(this).flexImages({
                rowHeight: height,
                truncate: truncate,
                container: "." + elementClass
            })
        });
    }
}

